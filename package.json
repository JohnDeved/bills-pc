{
  "name": "bills-pc",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "pokemon-save-parser": "bin/pokemon-save-parser",
    "mgba-docker": "node bin/mgba-docker"
  },
  "files": [
    "bin/",
    "src/lib/parser/",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "oxlint --fix .; biome format --write .",
    "format": "npx biome format --write .",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:website": "vitest run src/__tests__/health.test.ts src/__tests__/core-functionality.test.ts src/__tests__/integration-workflow.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:website && npm run build",
    "parse": "tsx src/lib/parser/cli.ts",
    "generate-mappings": "node scripts/generate-vanilla-mappings.js",
    "generate-icons": "tsx scripts/generate-icons.ts && tsx scripts/generate-og-image.ts",
    "mgba": "tsx docker/mgba-docker.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/fiber": "^9.4.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.7",
    "@types/react-query": "^1.2.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.24",
    "isomorphic-ws": "^5.0.0",
    "lucide-react": "^0.553.0",
    "next-themes": "^0.4.6",
    "pokeapi-types": "^0.0.2",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "three": "^0.181.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.181.0",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "@vitest/ui": "^4.0.8",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "cheerio": "^1.1.2",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "ox-standard": "github:JohnDeved/ox-standard",
    "oxlint": "^1.26.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sharp": "^0.34.5",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.8",
    "workbox-window": "^7.3.0",
    "ws": "^8.18.3"
  }
}
